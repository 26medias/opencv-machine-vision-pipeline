{
    "vision": {
        "pipeline": [{
            "op":       "box",
            "level":    "frame",
            "plugin":   "face"
        },{
            "op":       "attr",
            "level":    "object",
            "plugin":   "age",
            "result": {
                "type": "max",
                "maxSize": 50,
                "minSize": 50
            },
            "match": {
                "type": {
                    "$eq": "face"
                },
                "score": {
                    "$gt": 0.95
                },
                "age_inference_count": {
                    "$opt": true,
                    "$lt": 50
                }
            }
        }],
        "plugins": {
            "box": {
                "face": {
                    "import": "plugins.face.face",
                    "method": "getFacesFromFrame",
                    "enabled": true
                }
            },
            "attr": {
                "age": {
                    "import": "plugins.age.age",
                    "method": "getAgeFromFace",
                    "enabled": true
                }
            }
        }
    },
    "render": {
        "rules": [{
            "display": {
                "color": [0,179,255],
                "label": "([number]) [label] - Age Unknown",
                "box":   true
            },
            "match": {
                "type": {
                    "$eq": "face"
                },
                "score": {
                    "$gt": 0.7
                },
                "active": {
                    "$eq": true
                },
                "age": {
                    "$exists": false
                },
                "age_inference_count": {
                    "$opt": true,
                    "$lt": 200
                }
            }
        },{
            "display": {
                "color": [76,175,80],
                "label": "([number]) [label] - [age]",
                "box":   false
            },
            "match": {
                "type": {
                    "$eq": "face"
                },
                "score": {
                    "$gt": 0.7
                },
                "active": {
                    "$eq": true
                },
                "age": {
                    "$exists": true
                }
            }
        }]
    }
}